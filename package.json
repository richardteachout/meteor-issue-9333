    "storybook": "start-storybook -p 9001 -s ./public",
    "lint": "eslint --fix -c ./.eslintrc .",
    "ci-lint": "eslint -c ./.eslintrc imports",
    "test": "meteor test --raw-logs --driver-package practicalmeteor:mocha --port 5000 --settings settings-devlocal.json | node_modules/bunyan/bin/bunyan",
    "test-cli": "meteor test --once --driver-package dispatch:mocha-phantomjs --port 5050 --settings settings-devlocal.json",
    "test-setup": "npm i faker",
    "chimp-setup": "npm i chimp phantomjs faker",
    "chimp-watch-local": "SITE=http://localhost:3000 chimp --ddp=http://localhost:3000 --watch --path=tests/features",
    "chimp-watch-dev": "SITE=https://portal-dev.eltoro.com chimp --watch --path=tests/features",
    "chimp-watch-prod": "SITE=https://portal.eltoro.com chimp --watch --path=tests/features",
    "chimp-test-local": "SITE=http://localhost:3000 chimp --ddp=http://localhost:3000 --path=tests/features",
