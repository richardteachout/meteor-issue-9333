  "scripts": {
    "setup": "npm i -g bunyan",
    "start": "nice meteor --raw-logs --settings settings-devlocal.json | node_modules/bunyan/bin/bunyan",
    "cleanup": "rm -rf .meteor/local/bundler-cache/linker && rm -rf node_modules && meteor npm install",
    "start-with-email": "MAIL_URL='smtp://postmaster@mailgun.eltoro.com:b2163a7dad183ebb729088de403c7867@smtp.mailgun.org:587' meteor --settings settings-devlocal.json | node_modules/bunyan/bin/bunyan",
    "start-as-dev": "METEOR_OFFLINE_CATALOG=1 MONGO_URL='mongodb://10.0.2.168:27017/dev' meteor --raw-logs --settings settings-dev.json | node_modules/bunyan/bin/bunyan",
    "start-as-prod": "METEOR_OFFLINE_CATALOG=1 MONGO_URL='mongodb://10.0.2.202:27000,10.0.2.165:27000,10.0.2.129:27000/prod?replicaSet=prod' meteor --raw-logs --settings settings-prod.json | node_modules/bunyan/bin/bunyan",
